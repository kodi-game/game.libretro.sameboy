From cfa7f3ee45a682ebde4f99aeede864a4ebdbf940 Mon Sep 17 00:00:00 2001
From: Garrett Brown <themagnificentmrb@gmail.com>
Date: Sun, 11 Aug 2024 19:02:42 -0700
Subject: [PATCH] [temp] Print PATH var

---
 libretro/Makefile | 10 ++++++++--
 1 file changed, 8 insertions(+), 2 deletions(-)

diff --git a/libretro/Makefile b/libretro/Makefile
index 1169b98..9dd7be9 100644
--- a/libretro/Makefile
+++ b/libretro/Makefile
@@ -1,3 +1,11 @@
+# Prepend a target that prints the PATH
+print-path:
+	@echo "PATH: $(PATH)"
+	@exit 1
+
+# Ensure that the print-path target runs before anything else
+all: print-path $(TARGET)
+
 STATIC_LINKING := 0
 AR             := ar
 
@@ -363,8 +371,6 @@ endif
 
 CFLAGS   += -D__LIBRETRO__ $(fpic) $(INCFLAGS) -std=gnu11 -D_GNU_SOURCE -D_USE_MATH_DEFINES
 
-all: $(TARGET)
-
 $(CORE_DIR)/libretro/%_boot.c: $(CORE_DIR)/BootROMs/prebuilt/%_boot.bin
 	echo "/* AUTO-GENERATED */" > $@
 	echo "const unsigned char $(notdir $(@:%.c=%))[] = {" >> $@
-- 
